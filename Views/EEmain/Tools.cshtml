<script>
    $(function () {
        $("#btn-pdfmerge").click(function () {
            var urls = "";
            $(".txt-url").each(function (key, value) {
                if ($(this).val()) { urls = urls + $(this).val() + ","; }
            });
            alert("url" + urls);
            $.ajax({
                type: "POST",
                url: "/EEmain/CreatePDF",
                data: { "urls": urls },
                datatype: 'json',
                success: function (data) {
                    data = JSON.parse(data);
                    if (data.status == "ok") {
                        alert(data.msg);
                        //window.location = "/ExpertsManage/Index";
                    } else {
                        alert(data.msg);
                        //   RefreshCode();
                    }
                },
                error: function (xhr) {
                    alert("出现错误，请稍后再试：" + xhr.responseText);
                }
            });
        })
    });
</script>